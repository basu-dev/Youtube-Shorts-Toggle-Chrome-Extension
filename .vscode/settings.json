{
    "json.schemas": [
        {
            "fileMatch": ["manifest.json"],
            "url": "https://json.schemastore.org/chrome-manifest.json"
        }
    ],
    "files.exclude": {
        "dist/**":true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "search.exclude": {
        "dist/**":true
    }
}
